    vinput = 0;
}

//Dampin' n clampin'
hspd = clamp(hspd + hinput - dirx * damping, -movespeed, movespeed);
vspd = clamp(vspd + g, -maxspeed, maxspeed);

//Horizontal
dirx = sign(hspd);
if (place_meeting(x + hspd + dirx, y, ground)) {
    while (!place_meeting(x + dirx, y, ground)) x += dirx;
    hspd = 0;
}
x = clamp(x + hspd, 0, room_width);
dirx = sign(hspd);

//Vertical
diry = sign(vspd);
if (place_meeting(x, y + vspd + diry, ground)) {
    while (!place_meeting(x, y + diry, ground)) y += diry;
    vspd = vinput;
}
if (place_meeting(x, y + vspd + diry, enemy)) {
